/ {
	aliases {
		serial0 = &uart1;
		serial1 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&qspi {
	#address-cells = <1>;
	#size-cells = <0>;
	flash@0 {
		/delete-node/ partitions;
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;
			partition@0 {
				label = "Image Selector";
				reg = <0 0x60000>;
			};
			partition@60000 {
			        label = "Image Selector Backup";
			        reg = <0x60000 0x60000>;
			};
			partition@c0000 {
			        label = "Image Selector Scratchpad";
			        reg = <0xc0000 0x20000>;
			};
			partition@e0000 {
			        label = "Image Recovery";
			        reg = <0xe0000 0x1400000>;
			};
			partition@14e0000 {
			        label = "Image Recovery Scratchpad";
			        reg = <0x14e0000 0x20000>;
			};
			partition@1500000 {
			        label = "SystemReady-DT Update Metadata";
			        reg = <0x1500000 0x20000>;
			};
			partition@1520000 {
			        label = "SystemReady-DT Update Metadata Backup";
			        reg = <0x1520000 0x20000>;
			};
			partition@1540000 {
			        label = "U-Boot storage variables";
			        reg = <0x1540000 0x20000>;
			};
			partition@1560000 {
			        label = "U-Boot storage variables backup";
			        reg = <0x1560000 0x20000>;
			};
			partition@1580000 {
			        label = "Bank A Space";
			        reg = <0x1580000 0x1000000>;
			};
			partition@2580000 {
			        label = "U-Boot Variables Bank A";
			        reg = <0x2580000 0x20000>;
			};
			partition@25a0000 {
			        label = "U-Boot Variables Bank A Backup";
			        reg = <0x25a0000 0x20000>;
			};
			partition@25c0000 {
			        label = "Bank B Space";
			        reg = <0x25c0000 0x1000000>;
			};
			partition@35c0000 {
			        label = "U-Boot Variables Bank B";
			        reg = <0x35c0000 0x20000>;
			};
			partition@35e0000 {
			        label = "U-Boot Variables Bank B Backup";
			        reg = <0x35e0000 0x20000>;
			};
			partition@3600000 {
			        label = "User Scratchpad";
			        reg = <0x3600000 0xa00000>;
			};
		};
	};
};
